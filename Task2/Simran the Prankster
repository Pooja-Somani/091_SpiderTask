#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

bool check(vector<int>&v,double value,double result,unsigned index)
{
    if(index==v.size())
    {
        if(result==value)
        return true;
        return false;
    }    
    return (check(v,value+v[index],result,index+1) || check(v,value-v[index],result,index+1) || check(v,value*v[index],result,index+1) || (v[index]==0?false:check(v,value/v[index],result,index+1)));
    }

int main() {
    long long int t;
    cin>>t;       
    
    while(t--)
    {
        long long int n,result;
        cin>>n>>result;
        if (n==0)
        {
            cout<<"NO\n";
            continue;
        }        
        vector<int>v(n);
        for(int i=0;i<n;i++)
        cin>>v[i];
        
        if (n==1 && v[0]==result)
        {
            cout<<"YES\n";
            continue;
        }
        
        bool output=check(v,v[0],result,1);
        if (output==true)
            cout<<"YES\n";
        else
            cout<<"NO\n";
    }
    return 0;
}
