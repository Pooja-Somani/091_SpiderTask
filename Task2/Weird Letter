#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int t;
    cin>>t;
    while(t--)
    {
        string a, b;
        cin>>a;
        cin>>b;
        unsigned i=0, j=0, flag=1;
        
        while(i<a.size() && j<b.size())
        {
            if (a[i]==b[j])
            {
                i++;
                j++;
            }
            else if (i>0 && b[j]==a[i-1])
            {
                j++;
            }
            else 
            {
                flag=0;
                break;
            }                
        }
        
        if (i==a.size())
        {
            while(j<b.size())
            {
                if (b[j]!=a[i-1])
                {
                    flag=0;
                    break;
                }
                j++;
            }
        }
        
        if (j==b.size() && i<a.size())
        {
            flag=0;            
        }
        //cout<<flag<<endl;
        
        if (flag==1)
            cout<<"YES\n";
        else
            cout<<"NO\n";       
        
    }   
    return 0;
}
