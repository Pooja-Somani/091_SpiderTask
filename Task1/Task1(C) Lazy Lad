for i in range(int(input())):
    boxes=int(input())    
    rows=0
    count=1
    while(1):
        used=(count*(count+1))//2
        if (used>boxes):
            break;
        if (used==boxes):
            rows+=1;
            break;
        rows+=1;
        count+=1;
        

    print(rows)
            
